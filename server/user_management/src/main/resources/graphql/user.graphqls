type User {
    id: ID!
    userName: String!
    email: String!
    password: String!
}

input UserInput {
   userName: String!
    email: String!
    password: String!
}
type Query {
    getAllUsers: [User]
    getUsersById(id: ID!): User
}
type Mutation {
    createUser(userName: String!, email: String!, password: String!): User
    updateUser(userName: String!, email: String!, password: String!): User
    deleteUser(id: ID!): String
}

